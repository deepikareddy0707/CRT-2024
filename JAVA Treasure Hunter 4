import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
      int n,x,y;
      n=s.nextInt();
      x=s.nextInt();
      y=s.nextInt();
      int r1=n*x/100;
    System.out.printf("%d\n",r1);
    int r2=n-r1;
    int r3=r2*y/100;
    System.out.printf("%d\n",r3);
    int r4=(r2-r3)/3;
    System.out.printf("%d",r4);
    }
}
