import java.util.*;
class Test {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n,k;
        n=s.nextInt();
        k=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
        a[i]=s.nextInt();
        }
        Arrays.sort(a);
        System.out.println(a[k-1]);
    }
}
