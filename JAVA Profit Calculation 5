import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner s=new Scanner(System.in);
            int x,a,b;
            x=s.nextInt();
            a=s.nextInt();
            b=s.nextInt();
            int r1=(x*a)-(x*b)-100;
            System.out.println(r1);
    }
}
