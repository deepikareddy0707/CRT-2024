import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
      int a=10,b=20;
      a=s.nextInt();
      b=s.nextInt();
      int r=a*b;
    int k=a+b;
    if(r>1000)
    {
        System.out.printf("%d",k);
    }
    else
    {
       System.out.printf("%d",r);
    }
    }
}
